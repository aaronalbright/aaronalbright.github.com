{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true
    },
    "globals": {
        "$": true,
        "$1": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "comma-dangle": 0,
        "semi": [
            0,
            "always"
        ],
        "no-unused-vars": 1
    }
}